upstream upstream_name {
    server backend_server:80;
}
server {
    server_name server-name.domain.org;
    listen 80 ;
    access_log /var/log/nginx/access.log vhost;
    # for first run, enable the following line:
    include /etc/nginx/vhost.d/default;
    # once certificate was issued, you can comment above line and uncomment following line
    # return 301 https://$host$request_uri;
}
